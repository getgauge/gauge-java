{
    "id" : "java",
	"version" : "0.3.0",
    "description": "Java support for gauge",
	"run": {
		"windows": ["bin/gauge-java", "--start"],
		"linux": ["bin/gauge-java", "--start"],
		"darwin": ["bin/gauge-java", "--start"]
